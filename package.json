{
  "name": "keeb-bot",
  "version": "0.0.1",
  "author": "Natsuki Takahari",
  "description": "A bot for the 40s mechanical keyboard Discord server.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy-commands": "tsc && node build/commands/deploy-commands.js",
    "start": "tsc && node build/server.js",
    "watch": "concurrently \"tsc -w\" \"nodemon -e js build/server.js\"",
    "postinstall": "tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "@types/node-fetch": "^2.5.10",
    "axios": "^0.21.0",
    "discord-api-types": "^0.23.1",
    "discord.js": "^14.0.0-dev.1650672498-3617093",
    "node-cache": "^5.1.2",
    "slugify": "^1.4.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^16.3.1",
    "@types/ws": "^7.2.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "concurrently": "^6.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.4.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=16.6.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,json,md}": "prettier --write --ignore-unknown"
  }
}
